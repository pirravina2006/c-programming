#include<stdio.h>
int main(){
    int num;
    scanf("%d",&num);
    int rev=0;
    int temp=num;
    while(num!=0){
        int rem=num%10;
        rev=rev*10+rem;
        num=num/10;
    }
    if(rev==temp){
        printf("Number is palindrome");
    }
    else{
        printf("Number is not palindrome");
    }
    return 0;
}
